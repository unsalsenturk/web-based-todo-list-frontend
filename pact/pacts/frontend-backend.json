{
  "consumer": {
    "name": "Frontend"
  },
  "provider": {
    "name": "Backend"
  },
  "interactions": [
    {
      "description": "a request for getting all todo list",
      "providerState": "get todo list successfully",
      "request": {
        "method": "GET",
        "path": "/todolist"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "data": [
            {
              "id": 1,
              "description": "buy some milk"
            }
          ]
        },
        "matchingRules": {
          "$.body.data": {
            "min": 1
          },
          "$.body.data[*].*": {
            "match": "type"
          },
          "$.body.data[*].description": {
            "match": "type"
          }
        }
      }
    },
    {
      "description": "request to create to-do list",
      "providerState": "add todo list successfully",
      "request": {
        "method": "POST",
        "path": "/todolist",
        "body": {
          "todo": "buy some milk"
        },
        "matchingRules": {
          "$.body.todo": {
            "match": "type"
          }
        }
      },
      "response": {
        "status": 201,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "data": [
            {
              "id": 1,
              "description": "buy some milk"
            }
          ]
        },
        "matchingRules": {
          "$.body.data": {
            "min": 1
          },
          "$.body.data[*].*": {
            "match": "type"
          },
          "$.body.data[*].description": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}